!Xft.autohin:0
!Xft.antialias: 1
!Xft.hinting: true
!Xft.hintstyle: hintslight
!Xft.dpi:96
!Xft.rgba: rgb
!Xft.lcdfilter: lcddefault 

URxvt.cursorBlink: 1

! Disable scrollbar
URxvt.scrollBar: false

! Set terminal padding
!URxvt.internalBorder: 5

! Font configuration
URxvt*.depth: 32
URxvt*font:xft:DejaVuSansMono Nerd Font Mono:pixelsize=16, xft:DejaVu Sans:pixelsize=16
URxvt*letterSpace:0

! enable copy/paste using ctrl+shift+c and ctrl+shift+v
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755 : false

